{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Get Orchestrator Status Tool",
  "description": "MCP tool schema for retrieving simple orchestrator status",
  "type": "object",
  "properties": {
    "input": {
      "type": "object",
      "description": "No input parameters required for status check",
      "properties": {},
      "additionalProperties": false
    },
    "output": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string",
          "description": "Simple orchestrator status indicator",
          "enum": ["Alive"]
        }
      },
      "required": [
        "status"
      ],
      "additionalProperties": false
    }
  },
  "required": ["input", "output"]
}
